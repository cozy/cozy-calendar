{
  "name": "cozy-agenda",
  "version": "0.0.1",
  "description": "The application currently manages reminders and only reminders. It is based on the iCal standard.",
  "engines": [
    "node = 0.8.x"
  ],
  "description": "Agenda application for Cozy Cloud",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mycozycloud/cozy-agenda.git"
  },
  "main": "server.coffee",
  "dependencies": {
    "jade": "= 0.27.6",
    "express": "= 3.0.0",
    "connect": "= 2.6.1",
    "compound": "= 1.1.5-17",
    "coffee-script": "= 1.4.0",
    "mime": "= 1.2.7",
    "qs": "= 0.5.0",
    "jugglingdb": "= 0.2.0-23",
    "jugglingdb-cozy-adapter": "0.3.2",
    "async": "= 0.1.22",
    "cozy-realtime-adapter":"= 0.11.0",
    "redis": ">0",
    "time": "~0.9.2",
    "moment": "2.0.0"
  },
  "devDependencies": {
    "should": "= 1.2.0",
    "mocha": "= 1.6.0",
    "chai": "= 1.3.0",
    "request-json": ">= 0",
    "sinon": "~1.6.0"
  },
  "scripts": {
    "test": "cake tests",
    "start": "coffee server.coffee"
  },
  "cozy-permissions": {
    "Alarm": {
      "description": "Creates and edit your alarms."
      },
    "User": {
      "description": "Used to recover your timezone"
    }
  }
}
